prompt 1
default menu.c32
timeout 100

label 1. android_x86_64
menu android_x86_64
kernel android_x86_64/kernel
append ip=dhcp androidboot.selinux=permissive buildvariant=eng initrd=android_x86_64/initrd.img androidboot.hardware=x86 DEBUG=2 SRC=/android-x86vbox ROOT=/dev/sda1 VBOX_GUEST_ADDITIONS=1 SDCARD=vendor DATA=sda2 X86VBOX=1

label 2. x86vbox
menu x86vbox_ramdisk
kernel x86vbox/kernel
append ip=dhcp console=ttyS3,115200 androidboot.selinux=permissive initrd=x86vbox/ramdisk.img androidboot.hardware=x86vbox

label 3. Recovery - x86vbox
menu x86vbox_ramdisk_recovery
kernel x86vbox/kernel
append ip=dhcp console=ttyS3,115200 androidboot.selinux=permissive initrd=x86vbox/ramdisk-recovery.img androidboot.hardware=x86vbox

label 4. NFS Installation - x86vbox
menu x86vbox_install
kernel x86vbox/kernel
append ip=dhcp initrd=x86vbox/initrd.img root=/dev/nfs rw androidboot.hardware=x86vbox INSTALL=1 SRC=/x86vbox ROOT=10.0.2.2:/home/sgye/vol1/android-vbox/out/target/product VBOX_GUEST_ADDITIONS=1 SDCARD=vendor DATA=sda2

